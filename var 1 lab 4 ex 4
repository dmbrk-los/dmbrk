#include <stdio.h>
#include <math.h>
int main()
{
float x=0.05, y=0;
printf("\n*******************************\n");
printf("     X\t     \t   Y\n");
printf("*******************************\n");
while (x<=3)
{
y+=log10(x)/x;// якщо х<0, неможливо знайти логарифм, якщо х=0 ділити на 0 не можна, тому нижня межа не -1 а 0,05
printf("%f\t%7.2lf\n", x, y);
x+=0.05;
}
}
