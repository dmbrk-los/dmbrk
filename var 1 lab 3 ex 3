#include <stdio.h>
#include <math.h>

int main ()
{
float y,z,t;
printf("введіть у ");
scanf ("%f", &y);
printf("введіть z ");
scanf ("%f", &z);
t=sqrt(3*y/(z-2*y))-y*z;
if (z==2*y)
{
    printf("для заданих значень y та z у обчислюваному виразі виконується ділення на 0");
    return 0;
}
if ((3*y/(z-2*y))<0)
{
    printf("для заданих значень y та z у обчислюваному виразі виконується пошук корення з відємного числа");
    return 0;
}
printf("%f",t);
}
