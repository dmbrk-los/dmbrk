#include <stdio.h>
int main()
{
    int n;
    printf ("введіть n ");
    scanf("%d",&n);
    int a[n];
    printf ("введіть масив ");
    for (int i=0; i<n; i++)
    {
        scanf ("%d",&a[i]);
    }
    printf ("парні елементи масиву\n");
     for (int i=0; i<n; i++)
    {
        if (a[i]%2==0)
        printf ("%d\n",a[i]);
    }
     printf ("непарні елементи масиву менші 10-ти\n");
    for (int i=0; i<n; i++)
    {
        if (a[i]%2==1 && a[i]<10)
        {
            printf ("%d ",a[i]);
        }
    }
}
